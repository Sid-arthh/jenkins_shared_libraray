@Library("my-shared-lib") _
pipeline {
    agent any
    environment {
        AWS_REGION = "us-east-1"
        ECS_CLUSTER_NAME = 'SHARED_LIB'
        ECS_SERVICE_NAME = 'SERVICE2'
    }
    stages {
        stage('Build and Deploy') {
            steps {
                script {
                    // Call the wrapper function from the shared library
                    deployToECSWrapper()
                }
            }
        }
    }
}
